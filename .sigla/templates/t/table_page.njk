<jsp:useBean id="myBean" class="{{ bean }}" scope=request></jsp:useBean>
<html>
<head>
<title>{{ title }}</title>
</head>
<body>

<table>

<tr>
{% for child in children %}
<th>{{ child[ 'title' ] }}</th>
{% endfor %}
</tr>

% ResultSet result = myBean.query();
% while( result.next ) {
<tr>
{{ render(children) }}
</tr>
% }
</table>

</body>
</html>
